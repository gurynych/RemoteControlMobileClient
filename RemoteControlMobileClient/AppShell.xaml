<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="RemoteControlMobileClient.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:RemoteControlMobileClient"
       xmlns:pages="clr-namespace:RemoteControlMobileClient.Pages"
       xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
       xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
       Shell.FlyoutBehavior="Disabled"
       Shell.NavBarIsVisible="false"
       BindingContext="{x:Binding AppShellViewModel, Source={StaticResource vmLocator}}">
    <ShellContent Content="{pages:StartupPage}" />
</Shell>
